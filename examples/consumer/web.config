<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
    <compilation>
      <assemblies>
        <add assembly="Boo.Lang" />
        <add assembly="Janrain" />
        <add assembly="Janrain.OpenId.Consumer.Asp" />
        <add assembly="NerdBank.OpenId.AspNet" />
      </assemblies>
    </compilation>
    <authorization>
      <deny users="?"/>
    </authorization>

    <authentication mode="Forms">
      <forms name="OpenIdExampleConsumer" 
             loginUrl="~/login.aspx" requireSSL="false" />
    </authentication>
  </system.web>
  <appSettings>
    <add key="MonoServerDefaultIndexFiles" value="index.aspx" />
  </appSettings>
</configuration>

